@model List<Linguagens>




<h2>Linguagens</h2>
<a class="btn btn-link" asp-controller="Linguagens" asp-action="Create">Adicionar uma nova linguagem</a>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(Model => Model.First().Id)</th>
            <th>@Html.DisplayNameFor(Model => Model.First().Logo)</th>
            <th>@Html.DisplayNameFor(Model => Model.First().Nome)</th>
            <th>@Html.DisplayNameFor(Model => Model.First().Aplicacao)</th>
            <th>@Html.DisplayNameFor(Model => Model.First().Lancamento)</th>
            <th>@Html.DisplayNameFor(Model => Model.First().Desenvolvedor)</th>

        </tr>
    </thead>
    <tbody>
        @foreach (Linguagens l in Model)
        {
        <tr>
            <td> @Html.DisplayFor(Model => l.Id)</td>
            <td> @Html.DisplayFor(Model => l.Logo)</td>
            <td> @Html.DisplayFor(Model => l.Nome)</td>
            <td> @Html.DisplayFor(Model => l.Aplicacao)</td>
            <td> @Html.DisplayFor(Model => l.Lancamento)</td>
            <td> @Html.DisplayFor(Model => l.Desenvolvedor)</td>
        </tr>
        }
    </tbody>
</table>

